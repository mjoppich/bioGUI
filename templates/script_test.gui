<template description="Script Test" title="Script Test">
 <window title="Trimmomatic" >
  <vgroup>
    <action id="action">action</action>
  </vgroup>
  </window>
  <execution>
    <const id="const_node">this is a const value</const>

    <script id="cl" argv="${const_node},bla">
    <![CDATA[
function evaluate(a, b)
    local some_var = 'This is coming from lua.tellme. ' .. a .. b
    local final_str = '\"' .. some_var .. '\"'

    local returnval = string.sub(final_str, 6, -1)

    return returnval
end
print("Priming run")
    ]]>
    </script>


  <execute param="${cl}" location="" exec="echo" test="false">

  </execute>

  </execution>
  </template>